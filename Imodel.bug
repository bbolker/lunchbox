model{
  ## Initial values
    I <- numeric(n)
    I[1] <- i0
    Pi[1] <- 1 - (1-beta)^I[1]
    inc[1] ~ dbin(Pi[1],N-sum(inc))
    
    ## Step through observations
    for (t in 2:M) {
        I[t] <- I[t-1] + inf[t-1]
        
        Pi[t] <- 1 - (1-beta)^I[t]
        inf[t] ~ dbin(Pi[t],N-sum(inc)+1)
    }
    ## aux variables
    
    ## priors
    beta ~ dunif(0,2)

}
